<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="fr.obeo.dsl.arduino.branding.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="fr.obeo.dsl.arduino.branding.application"
            name="Arduino Designer">
         <property
               name="appName"
               value="Arduino Designer">
         </property>
          <property name="preferenceCustomization" value="plugin_customization.ini"/>
      </product>
   </extension>

</plugin>
